  "private": true,
  "dependencies": {
    "apollo-boost": "^0.1.28",
    "graphql": "^14.1.1",
    "proptypes": "^1.1.0",
    "react": "^16.8.3",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
